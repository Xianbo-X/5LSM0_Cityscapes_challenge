{
    "architecture": {
        "prefix": "scripts.models.res_unet.res_unet",
        "model": "Res_UNet",
        "param": {
            "n_channels": 3,
            "n_classes": 30
        }
    },
    "param": {
        "learning_rate": 0.0002,
        "epochs": 15,
        "batch_size": 5,
        "aug_mode": "Both",
        "save_inter_model": false
    },
    "augmentation": {
        "transformation": {
            "prefix": "scripts.data_augmentation",
            "transforms": [
                {
                    "name": "crop.RandomCropAndResize",
                    "param": {
                        "pr": 1,
                        "width_lim": [
                            30,
                            250
                        ],
                        "height_lim": [
                            30,
                            120
                        ]
                    }
                },
                {
                    "name": "flip.RandomHorizontalFlip2",
                    "param": {
                        "pr": 1
                    }
                },
                {
                    "name": "pixels_change.RandomRgb2Gray2",
                    "param": {
                        "pr": 1
                    }
                },
                {
                    "name": "rotate.RandomRotation",
                    "param": {
                        "pr": 1
                    }
                }
            ]
        }
    },
    "path": {
        "ROOT_PATH": "saves",
        "SAVE_PATH": "Res_UNet-lr-0p0002-AUGMODE_Both-EXP_1",
        "MODEL_PATH": "models",
        "CHECK_POINT": "",
        "MODEL_PREFIX": "Res_UNet",
        "RESULT_PATH": "res"
    }
}